<!DOCTYPE web-app PUBLIC
  '-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN'
  'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.fsl.fslclubs.servlet.LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>activities</servlet-name>
    <servlet-class>com.fsl.fslclubs.servlet.ActivityServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>activities</servlet-name>
    <url-pattern>/activities</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>signup_activities</servlet-name>
    <servlet-class>com.fsl.fslclubs.servlet.SignupActivityServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>signup_activities</servlet-name>
    <url-pattern>/signup_activities</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>check_club_members</servlet-name>
    <servlet-class>com.fsl.fslclubs.servlet.CheckClubMemberServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>check_club_members</servlet-name>
    <url-pattern>/check_club_members</url-pattern>
  </servlet-mapping>
</web-app>