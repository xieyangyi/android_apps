<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fsl.fslclubs" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".login.LoginActivity" android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.RegisterActivity"></activity>

        <activity android:name=".main.MainActivity"></activity>

        <activity android:name=".activities.ActivitiesActivity"></activity>

        <activity android:name=".friends.MyActivityActivity"></activity>
        <activity android:name=".friends.AlarmActivity"></activity>

        <activity android:name=".clubs.ClubsActivity"></activity>

        <activity android:name=".me.AboutActivity"></activity>
        <activity android:name=".me.SettingsActivity"></activity>
        <activity android:name=".me.PersonInfoActivity"></activity>

        <provider android:name=".login.UserInfoProvider"
            android:authorities="com.fsl.fslclubs.provider" />
        <provider android:name=".friends.MyActivityProvider"
            android:authorities="com.fsl.fslclubs.myactivity.provider" />

        <receiver android:name=".friends.AlarmReceiver">
            <intent-filter>
                <action android:name="com.fsl.fslclubs.AlarmReceiver" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
</manifest>
